{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "relview",
      "camelcase": "RelView",
      "title": "RelView",
      "scope": "source.relview",
      "file-types": [
        "prog"
      ],
      "injection-regex": "^relview$",
      "class-name": "TreeSitterRelview"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "RelView program language",
    "authors": [
      {
        "name": "Benjamin Davies",
        "email": "bentendavies@gmail.com",
        "url": "https://bend.nz/"
      }
    ],
    "links": {
      "repository": "https://github.com/Benjamin-Davies/tree-sitter-relview"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}