{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "relview_relation",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "line"
          }
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": " ("
        },
        {
          "type": "SYMBOL",
          "name": "domain"
        },
        {
          "type": "STRING",
          "value": ")\n"
        }
      ]
    },
    "domain": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": ", "
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": " : "
        },
        {
          "type": "SYMBOL",
          "name": "number_list"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "number_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ", "
              },
              {
                "type": "SYMBOL",
                "name": "number"
              }
            ]
          }
        }
      ]
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}